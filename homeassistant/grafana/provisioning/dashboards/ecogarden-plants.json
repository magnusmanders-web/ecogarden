        {
          "datasource": {
            "type": "influxdb",
            "uid": "${DS_INFLUXDB}"
          },
          "query": "from(bucket: \"homeassistant\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"plant_health\")\n  |> filter(fn: (r) => r[\"_field\"] == \"health_score\")\n  |> filter(fn: (r) => r[\"plant_name\"] != \"_overall\")\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)\n  |> yield(name: \"mean\")",
          "refId": "A"
        }
      ],
      "title": "Plant Health Over Time",
      "type": "timeseries"
    },
